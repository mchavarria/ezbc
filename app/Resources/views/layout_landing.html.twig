{% extends 'base.html.twig' %}

{% block title %}{{ parent() }}{% endblock %}

{% block custom_stylesheets %}
    <!-- ================== BEGIN BASE CSS STYLE ================== -->
    <link href="{{ asset('plugins/landing/bootstrap3/css/bootstrap.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('plugins/landing/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('plugins/animate/animate.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('css/landing/style.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('css/landing/style-responsive.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('css/landing/theme/default.css') }}" id="theme" rel="stylesheet" />
    <!-- ================== END BASE CSS STYLE ================== -->
    <!-- ================== BEGIN BASE JS ================== -->
    <script src="{{ asset('plugins/pace/pace.min.js') }}"></script>
    <!-- ================== END BASE JS ================== -->
{% endblock %}

{% block custom_scripts %}
    <!-- ================== BEGIN BASE JS ================== -->
    <script src="{{ asset('plugins/jquery/jquery-3.2.1.min.js') }}"></script>
    <script src="{{ asset('plugins/landing/bootstrap3/js/bootstrap.min.js') }}"></script>
    <!--[if lt IE 9]>
    <script src="{{ asset('crossbrowserjs/html5shiv.js') }}"></script>
    <script src="{{ asset('crossbrowserjs/respond.min.js') }}"></script>
    <script src="{{ asset('crossbrowserjs/excanvas.min.js') }}"></script>
    <![endif]-->
    <script src="{{ asset('plugins/js-cookie/js.cookie.js') }}"></script>
    <script src="{{ asset('plugins/landing/scrollMonitor/scrollMonitor.js') }}"></script>
    <script src="{{ asset('js/landing/apps.min.js') }}"></script>
    <!-- ================== END BASE JS ================== -->
    <script>
        $(document).ready(function() {
            App.init();
        });
    </script>
{% endblock %}


{% block body_attributes %}data-spy="scroll" data-target="#header-navbar" data-offset="51"{% endblock %}

{% block page_container %}
    <!-- begin #page-container -->
    <div id="page-container" class="fade">
        <!-- begin #header -->
        {% include 'landing/header.html.twig' %}
        <!-- end #header -->

        {% block content %}
        {% endblock %}

        <!-- begin #footer -->
        {% include 'landing/footer.html.twig' %}
        <!-- end #footer -->
    </div>
    <!-- end #page-container -->
{% endblock %}